pipeline {
    agent any
environment {
PATH = "/bin:&PATH"
}
    stages 
    {
        stage("git checkout") 
        {
            steps
            {
                git branch: 'main', credentialsId: 'github', url: 'https://github.com/maheshprathipati/REPO.git'
            }
        }
stage("build build process") {
steps{
sh "mvn clean package"
}
}
stage ("deploying to test system") {
steps {
sh "scp target/gamutkart.war ubuntu@172.31.16.76:/home/ubuntu/tm/apache-tomcat-9.0.94/webapps"
}
}

    }
}
